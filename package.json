{
  "name": "mithril-onclickoutside",
  "version": "1.0.0",
  "description": "A wrapper component which takes care of clicks outside an element. Useful for scenarios when one might want to close something like a dropdown menu on clicking outside the menu",
  "main": "dist/mithril-onclickoutside.cjs.js",
  "module": "dist/mithril-onclickoutside.esm.js",
  "browser": "dist/mithril-onclickoutside.umd.js",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "jest": "^26.4.2",
    "mithril": "^2.0.4",
    "mithril-testing-library": "^1.0.4",
    "rollup": "^1.29.0",
    "rollup-plugin-peer-deps-external": "^2.2.3"
  },
  "peerDependencies": {
    "mithril": "1.x || 2.x"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest"
  },
  "keywords": ["mithril", "mithriljs", "onclickoutside", "clickoutside"],
  "author": "Mukesh Soni",
  "license": "MIT"
}
